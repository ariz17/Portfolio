{
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*\\.(mp4|webm|ogg))",
      "headers": {
        "Content-Type": "video/mp4",
        "Accept-Ranges": "bytes",
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
